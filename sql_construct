CREATE TABLE `fund_basic` (
  `id` int(10) unsigned NOT NULL AUTO_INCREMENT,
  `fund_code` varchar(255) DEFAULT NULL,
  `fund_name` varchar(255) DEFAULT NULL,
  `today` date DEFAULT NULL,
  `dot` decimal(10,4) DEFAULT NULL,
  `price_addtion` decimal(10,4) DEFAULT NULL,
  `price` decimal(10,4) DEFAULT NULL,
  `earn_per` decimal(10,2) DEFAULT NULL,
  `cost_all` int(11) DEFAULT NULL,
  `now_all` int(11) DEFAULT NULL,
  PRIMARY KEY (`id`)
) ENGINE=InnoDB AUTO_INCREMENT=159376 DEFAULT CHARSET=utf8